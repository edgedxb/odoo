<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

         <!-- Sequences for Contracts -->
        <record id="seq_view_contract" model="ir.sequence">
            <field name="name">AMC Contract Sequence</field>
            <field name="code">amc.contract</field>
            <field name="prefix">AMC/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
