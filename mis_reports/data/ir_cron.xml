<odoo>
    <data>
        <record id="ir_cron_module_notpaid_invoice_email" model="ir.cron">
            <field name="name">Not Paid Invoice</field>
            <field name="active" eval="True"/>
            <field name="state" eval="'code'"/>
            <field name="priority" eval="5"/>
            <field name="interval_number">1</field>
            <field name="interval_type">Day</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="mis_reports.model_notpaid_invoices"/>
            <field name="code" eval="'model._send_notpaid_email_notification()'"/>
        </record>

        <record id="ir_cron_module_notpaid_invoice_email_summary" model="ir.cron">
            <field name="name">Not Paid Invoice Summary</field>
            <field name="active" eval="True"/>
            <field name="state" eval="'code'"/>
            <field name="priority" eval="5"/>
            <field name="interval_number">1</field>
            <field name="interval_type">Day</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="mis_reports.model_notpaid_invoices"/>
            <field name="code" eval="'model._send_notpaid_email_summary_notification()'"/>
        </record>


    </data>
</odoo>
